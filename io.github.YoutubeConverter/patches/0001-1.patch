 Youtube.desktop | 10 ++++++++++
 Youtube.pro     | 11 +++++++++++
 2 files changed, 21 insertions(+)
 create mode 100644 Youtube.desktop

diff --git a/Youtube.desktop b/Youtube.desktop
new file mode 100644
index 0000000..0f3dafd
--- /dev/null
+++ b/Youtube.desktop
@@ -0,0 +1,10 @@
+[Desktop Entry]
+Categories=Game;Qt;
+Exec=Youtube
+Name=Youtube
+StartupNotify=false
+Terminal=false
+Type=Application
+X-Deepin-Vendor=user-custom
+
+Icon=yt
diff --git a/Youtube.pro b/Youtube.pro
index 20d6291..8c62211 100644
--- a/Youtube.pro
+++ b/Youtube.pro
@@ -28,3 +28,14 @@ FORMS += \
 qnx: target.path = /tmp/$${TARGET}/bin
 else: unix:!android: target.path = /opt/$${TARGET}/bin
 !isEmpty(target.path): INSTALLS += target
+
+BINDIR = $$PREFIX/bin
+DATADIR = $$PREFIX/share
+target.path = $$BINDIR
+desktop.files =Youtube.desktop
+desktop.path = $$DATADIR/applications/
+INSTALLS += target desktop
+
+icons.path = $${PREFIX}/share/icons
+icons.files = yt.png
+INSTALLS += icons
\ No newline at end of file
